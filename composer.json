{
    "name": "rhurup/laravel-admin-media-manager",
    "description": "Media manager for laravel",
    "type": "library",
    "keywords": ["laravel-admin", "media", "manager"],
    "homepage": "https://github.com/rhurup/laravel-admin-media-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Rasmus Hurup Hansen",
            "email": "rasmus@huruphansen.dk"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "encore/laravel-admin": "~1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Admin\\Media\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Admin\\Media\\MediaServiceProvider"
            ]
        }
    }
}
